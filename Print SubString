# Printing sub string

s='abcd'

for i in range(len(s)):
    for j in range(i+1,len(s)+1):
        print(s[i:j],end=' ')
    print('\n')


# Rotated String

a="NETSET"
t=a+a
for i in range(len(a)):
    for j in range(len(a)):
        print(t[i+j],end='')
    print("\n")

# Remove Duplicate in Dictionary:

dic={'car':['Ford','Toyota','Ford','Ford'],'brand':['Tata','Suzuki','Tata','Suzuki']}

for k,v in dic.items():
    dic[k]=set(v)

print(dic)